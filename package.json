{
  "name": "rgb-eleventy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Brian Wisti <brianwisti@pobox.com>",
  "license": "MIT",
  "scripts": {
    "site:img": "ts-node build_tasks/process_images.ts",
    "site:content": "eleventy --quiet",
    "site:style": "sass ./src/assets/scss/main.scss ./dist/assets/css/main.css",
    "site:prod": "INCLUDE_ANALYTICS=1 eleventy --quiet",
    "serve": "browser-sync start --server \"dist\" --files \"dist\" --no-open",
    "watch:site": "eleventy --watch --quiet",
    "watch:style": "sass --watch ./src/assets/scss/main.scss ./dist/assets/css/main.css",
    "watch": "npm-run-all --parallel serve watch:*",
    "push": "rsync -av dist/ vps:randomgeekery.org",
    "logs:fetch": "rsync -rLtv --exclude='*analog*' --exclude='.htpasswd' vps:logs/randomgeekery.org/ ~/Dropbox/logs/randomgeekery.org",
    "logs:load": "node build_tasks/process_logs.mjs"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@small-tech/jsdb": "^2.0.6",
    "@types/fs-plus": "^3.0.2",
    "@types/node": "^17.0.5",
    "alpine": "^0.2.1",
    "eleventy-plugin-shiki-twoslash": "^1.0.43",
    "fast-glob": "^3.2.7",
    "fs-plus": "^3.1.1",
    "luxon": "^2.2.0",
    "markdown-it": "^12.3.0",
    "markdown-it-attrs": "^4.1.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "sass": "^1.45.1",
    "sharp": "^0.29.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "xregexp": "^5.1.0"
  }
}