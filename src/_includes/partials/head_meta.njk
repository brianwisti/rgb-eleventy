<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link
  href="/index.xml"
  rel="alternate"
  type="application/rss+xml"
  title="Random Geekery Article Feed" />
<link
  href="/post/index.xml"
  rel="alternate"
  type="application/rss+xml"
  title="Random Geekery Post Feed" />
<link
  href="/note/index.xml"
  rel="alternate"
  type="application/rss+xml"
  title="Random Geekery Note Feed" />
<meta property="og:url" content="{{ url }}">
<meta property="og:title" content="{{ title }}">
<meta name="twitter:title" content="{{ title }}">
<meta name="twitter:site" content="@brianwisti">
{% if description %}
  <meta name="twitter:description" content="{{ description }}">
  <meta name="og:description" content="{{ description }}">
{% endif %}
<link
  rel="webmention"
  href="https://webmention.io/randomgeekery.org/webmention" />
<link
  rel="pingback"
  href="https://webmention.io/randomgeekery.org/xmlrpc" />
{% plausible %}